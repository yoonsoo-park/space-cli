{
  "name": "space-cli",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "Yoonsoo Park",
  "bin": {
    "space-cli": "./bin/run"
  },
  "homepage": "https://github.com/yoonsoo-park/space-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "yoonsoo-park/space-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.22.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.8"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.15",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.10",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "~4.7.0",
    "gts": "^3.1.1"
  },
  "oclif": {
    "bin": "space-cli",
    "dirname": "space-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "gts lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/yoonsoo-park/space-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
